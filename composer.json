{
  "name": "rossity/laravel-json-api-scaffolder",
  "description": "A highly opinionated package that rapidly scaffolds Laravel JSON API projects.",
  "license": "MIT",
  "authors": [
    {
      "name": "Ross Ball",
      "email": "w.ross.ball@gmail.com",
      "homepage": "https://github.com/rossity"
    }
  ],
  "homepage": "https://github.com/rossity/laravel-json-api-scaffolder",
  "keywords": [
    "laravel",
    "api",
    "json",
    "json-api"
  ],
  "require": {
    "php": "^7.4",
    "illuminate/support": "~7|~8",
    "spatie/laravel-query-builder": "^3.2",
    "spatie/laravel-json-api-paginate": "^1.8",
    "nette/php-generator": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Rossity\\LaravelApiScaffolder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rossity\\LaravelApiScaffolder\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rossity\\LaravelApiScaffolder\\LaravelApiScaffolderServiceProvider"
      ],
      "aliases": {
        "LaravelApiScaffolder": "Rossity\\LaravelApiScaffolder\\Facades\\LaravelApiScaffolder"
      }
    }
  }
}
